     A            CCDTEX         7P 0       TEXT('EXPIRE DATE')
     A                                      COLHDG('Expire Date')
     A                                      EDTCDE(Y)
     A            CC$CST         7P 2       TEXT('CERT. COST')
     A                                      COLHDG('Cert. Cost')
     A                                      EDTCDE(1)
     A            CCVEND         6S 0       TEXT('VENDOR #')
     A                                      COLHDG('Vend #')
     A                                      EDTCDE(4)
     A            CCDTLA         6S 0       TEXT('DATE LAST CHANGED')
     A                                      COLHDG('Date Last')
     A                                      EDTCDE(Y)
     A            CCTIME         6S 0       TEXT('TIME LAST CHANGED')
     A                                      COLHDG('Time Last')
     A                                      EDTCDE(Y)
     A            CCUSER        10A         TEXT('USER I.D.')
     A                                      COLHDG('User I.D.')
      *
     A          K CCCERT

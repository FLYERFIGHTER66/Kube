     A            CCDTRC         7P 0       TEXT('DATE VOID/REDEEM')
     A                                      COLHDG('Date Void/Redeemed')
     A                                      EDTCDE(Y)
     A            CC$AMT         7P 2       TEXT('CERT. AMOUNT')
     A                                      COLHDG('Cert. Amount')

     A                                      EDTCDE(4)
     A            CCCUST         6S 0       TEXT('NEW CUST#')
     A                                      COLHDG('New Cust#')
     A                                      EDTCDE(4)
      ** STATUS A=ACTIVE  V=VOIDED  R=REDEEMED  E=EXPIRED
     A            CCSTAT         1A         TEXT('STATUS')
     A                                      COLHDG('Status')
      ** TYPE T=TRAVEL  C=COMPUTER
     A            CCTYPE         1A         TEXT('TYPE')
     A                                      COLHDG('Type')
     A            CCDTIS         7P 0       TEXT('DATE ISSUED CYMD')
     A                                      COLHDG('Date Issued CYMD')

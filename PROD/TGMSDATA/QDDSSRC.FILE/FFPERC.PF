     A                                      COLHDG('Status')
     A            FPCONO         2S 0       TEXT('COMPANY NUMBER')
     A                                      COLHDG('Co' '#')
     A                                      EDTCDE(3)
     A            FPCODE         6A         TEXT('FREIGHT CODE')
